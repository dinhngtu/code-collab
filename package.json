{
	"name": "vscode-teletype",
	"displayName": "vscode-teletype",
	"description": "VS Code Teletype extension for Creating CoEditing skeleton for Eclipse Che and Theia",
	"types": "dist/index.d.ts",
	"publisher": "redhat",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/Rijul5/vscode-teletype.git"
	},
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.join-portal",
		"onCommand:extension.create-portal",
		"onCommand:extension.yjs-websocket"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.join-portal",
				"title": "Join Portal"
			},
			{
				"command": "extension.create-portal",
				"title": "Create Portal"
			},
			{
				"command": "extension.yjs-websocket",
				"title": "Join YJS Websocket Room"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.17.24",
		"@types/temp": "^0.9.0",
		"awesome-typescript-loader": "^5.2.1",
		"deep-equal": "^1.1.1",
		"memfs": "^3.2.2",
		"source-map-loader": "^0.2.4",
		"temp": "^0.9.4",
		"ts-mockito": "^2.6.1",
		"tslint": "^5.20.1",
		"typescript": "^3.9.5",
		"unionfs": "^4.4.0",
		"vsce": "~1.71.0",
		"vscode": "^1.1.37",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"@atom/teletype-client": "github:Rijul5/teletype-client",
		"@types/uuid": "^8.3.0",
		"google-protobuf": "^3.12.2",
		"mkdirp-promise": "^5.0.1",
		"node-fetch": "^2.6.0",
		"queue-typescript": "^1.0.1",
		"uuid": "^8.3.2",
		"wrtc": "^0.4.5",
		"ws": "^7.4.5",
		"y-websocket": "^1.3.15",
		"yjs": "^13.5.6"
	}
}
